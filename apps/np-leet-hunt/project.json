{
    "name": "np-leet-hunt",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/np-leet-hunt/src",
    "prefix": "np",
    "targets": {
        "build": {
            "executor": "@angular-devkit/build-angular:browser",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/apps/np-leet-hunt",
                "index": "apps/np-leet-hunt/src/index.html",
                "main": "apps/np-leet-hunt/src/main.ts",
                "polyfills": "apps/np-leet-hunt/src/polyfills.ts",
                "tsConfig": "apps/np-leet-hunt/tsconfig.app.json",
                "inlineStyleLanguage": "scss",
                "assets": [
                    "apps/np-leet-hunt/src/assets",
                    {
                        "glob": "**/*.svg",
                        "input": "node_modules/ionicons/dist/ionicons/svg",
                        "output": "./svg"
                    },
                     {
                        "glob": "**/*",
                        "input": "libs/np-phaser/src/assets",
                        "output": "./assets"
                    }
                ],
                "styles": ["apps/np-leet-hunt/src/global.scss"],
                "scripts": []
            },
            "configurations": {
                "production": {
                    "budgets": [
                        {
                            "type": "initial",
                            "maximumWarning": "2mb",
                            "maximumError": "3mb"
                        },
                        {
                            "type": "anyComponentStyle",
                            "maximumWarning": "2kb",
                            "maximumError": "4kb"
                        }
                    ],
                    "fileReplacements": [
                        {
                            "replace": "apps/np-leet-hunt/src/environments/environment.ts",
                            "with": "apps/np-leet-hunt/src/environments/environment.prod.ts"
                        }
                    ],
                    "outputHashing": "all"
                },
                "development": {
                    "buildOptimizer": false,
                    "optimization": false,
                    "vendorChunk": true,
                    "extractLicenses": false,
                    "sourceMap": true,
                    "namedChunks": true
                }
            },
            "defaultConfiguration": "production"
        },
        "serve": {
            "executor": "@angular-devkit/build-angular:dev-server",
            "configurations": {
                "production": {
                    "browserTarget": "np-leet-hunt:build:production"
                },
                "development": {
                    "browserTarget": "np-leet-hunt:build:development"
                }
            },
            "defaultConfiguration": "development"
        },
        "extract-i18n": {
            "executor": "@angular-devkit/build-angular:extract-i18n",
            "options": {
                "browserTarget": "np-leet-hunt:build"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": ["apps/np-leet-hunt/src/**/*.ts", "apps/np-leet-hunt/src/**/*.html"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/apps/np-leet-hunt"],
            "options": {
                "jestConfig": "apps/np-leet-hunt/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "stylelint": {
            "executor": "nx-stylelint:lint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["apps/np-leet-hunt/**/*.css", "apps/np-leet-hunt/**/*.scss"]
            }
        }
    },
    "tags": []
}
